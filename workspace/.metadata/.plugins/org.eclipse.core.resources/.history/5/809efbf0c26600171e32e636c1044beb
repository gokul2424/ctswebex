package com.cog.enitity;

public class User {
	
	int id;
	String name;
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "User [id=" + id + ", name=" + name + "]";
	}

	public User(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

}
